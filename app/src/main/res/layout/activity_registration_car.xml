<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nestedScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="ltr"
    android:paddingRight="15dp"
    android:paddingLeft="15dp"
    android:background="@drawable/splash"
   >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="match_parent"
        android:paddingBottom="20dp"
        android:layout_height="wrap_content">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"

            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:textSize="20dp"
            android:textColor="#c3493a"
            android:layout_gravity="center_horizontal"
            android:text="@string/text_register"
            />
        <ImageView
            android:layout_width="100dp"

            android:layout_height="100dp"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:background="@drawable/useradd"
            android:layout_gravity="center"
            android:id="@+id/adduser"
            />
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayoutName"
            android:background="@drawable/shape"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/textInputEditTextName"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="@string/hint_name"
                android:inputType="text"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginTop="10dp"
            android:weightSum="4"
            >

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayoutphone"
                android:background="@drawable/shape"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="4"
                >

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/textInputEditTextphone"
                    android:layout_width="match_parent"
                    android:background="@null"
                    android:layout_height="30dp"
                    android:hint="@string/hint_phone"

                    android:inputType="number"
                    android:maxLines="1"
                    android:textColor="@color/colorAccent" />
            </com.google.android.material.textfield.TextInputLayout>

        </LinearLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayoutaddress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/textInputEditTextaddress"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="@string/address"
                android:background="@null"
                android:inputType="text"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayoutlocation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape"
            android:layout_marginTop="10dp">

            <TextView
                android:id="@+id/textInputEditTextlocation"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:hint="@string/location"
                android:gravity="center_vertical"
                android:background="@null"
                android:inputType="text"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayout"
            android:background="@drawable/shape"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/textInputEditTextPassword"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="@string/carNumber"
                android:inputType="text"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayoutc"
            android:background="@drawable/shape"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/textInputEditTextcarmodel"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="@string/carModel"
                android:inputType="text"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginTop="10dp"
            android:weightSum="4"
            >

        <LinearLayout
            android:id="@+id/textInputLayoutcity"
            android:background="@drawable/shape"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            >

            <Spinner
                android:id="@+id/textInputEditTextcity"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="@string/country"
                android:layout_gravity="left"
                android:gravity="left"
                android:inputType="text"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </LinearLayout>
        </LinearLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayouts"
            android:background="@drawable/shape"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/textInputEditTexts"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="ادخل الحي"
                android:inputType="text"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayoutss"
            android:background="@drawable/shape"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/textInputEditTextss"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="ادخل الشارع"
                android:inputType="text"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayoutPassword"
            android:background="@drawable/shape"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/textInputEditTextPasswordd"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:hint="@string/hint_password"
                android:inputType="textPassword"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/colorAccent" />
        </com.google.android.material.textfield.TextInputLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:weightSum="2"
    >
<LinearLayout
    android:layout_width="0dp"
    android:layout_height="130dp"
    android:layout_weight="1"
    android:weightSum="12"
    android:orientation="vertical"
    >
    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:gravity="center"
        android:layout_gravity="center"
        android:textColor="#ffffff"
        android:padding="1dp"

        android:textStyle="bold"
        android:text="صورة المركبة"
        />
    <ImageView
        android:layout_width="120dp"
        android:layout_weight="9"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/unnamed"
        android:layout_gravity="center"
        android:id="@+id/imagecar"
        />
</LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="130dp"
            android:layout_weight="1"
            android:weightSum="12"
            android:orientation="vertical"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="3"
                android:layout_gravity="center"
                android:padding="1dp"
                android:textColor="#ffffff"
                android:textStyle="bold"

                android:gravity="center"
                android:text="@string/personId"
                />
    <ImageView
        android:layout_width="match_parent"
        android:layout_weight="9"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/car"
        android:layout_gravity="center"
        android:id="@+id/imageid"
        />
        </LinearLayout>
</LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/appCompatButtonRegister"
            android:background="@drawable/shapelogin"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"

            android:textColor="#000000"
            android:text="@string/text_register" />

        <!--<androidx.appcompat.widget.AppCompatButton-->
            <!--android:id="@+id/appCompatButtonFace"-->
            <!--android:background="@drawable/shape2"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_gravity="center"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginTop="10dp"-->

            <!--android:textColor="#ffffff"-->
            <!--android:text="@string/face" />-->
    </androidx.appcompat.widget.LinearLayoutCompat>
    </RelativeLayout>
    </ScrollView>
</androidx.core.widget.NestedScrollView>